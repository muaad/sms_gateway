<h1>Listing sender_ids</h1>

<table>
  <thead>
    <tr>
      <th>Aggregator</th>
      <th>Sender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sender_ids.each do |sender_id| %>
      <tr>
        <td><%= sender_id.aggregator %></td>
        <td><%= sender_id.sender_id %></td>
        <td><%= link_to 'Show', sender_id %></td>
        <td><%= link_to 'Edit', edit_sender_id_path(sender_id) %></td>
        <td><%= link_to 'Destroy', sender_id, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sender', new_sender_id_path %>
